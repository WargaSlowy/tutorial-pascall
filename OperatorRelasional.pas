program OperatorRelasional;

var
    nilai_pertama: integer;
    nilai_kedua: integer;

begin
    nilai_pertama := 10;
    nilai_kedua := 7;

    writeln(nilai_pertama > nilai_kedua);
    writeln(nilai_pertama < nilai_kedua);
    writeln(nilai_pertama = nilai_kedua);
    writeln(nilai_pertama <> nilai_kedua);
    writeln(nilai_pertama >= nilai_kedua);
    writeln(nilai_pertama <= nilai_kedua);
    writeln(7 <= 5);
end.